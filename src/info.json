{
  "name": "bp100",
  "version": "0.32.8",
  "title": "Staged Blueprint Planning",
  "description": "The ultimate mod for designing staged blueprints.",
  "homepage": "https://github.com/GlassBricks/StagedBlueprintPlanning/",
  "author": "GlassBricks",
  "factorio_version": "1.1",
  "package": {
    "readme": "mod-portal/readme.md",
    "gallery": [
      "mod-portal/gallery/Highlights.png",
      "mod-portal/gallery/Entity-info.png",
      "mod-portal/gallery/A0.png",
      "mod-portal/gallery/A1.png",
      "mod-portal/gallery/A2.png",
      "mod-portal/gallery/A3.png"
    ],
    "scripts": {
      "compile": "npm run build:release",
      "prepackage": "npm run prepackage",
      "prepublish": "npm run check && npm run subst-current-version",
      "postpackage": "npm run postpackage"
    },
    "git_publish_branch": "releases",
    "ignore": ["**/test/**/*", "**/*.xcf", "**/*.svg", "mod-portal/**/*", "**/*.ts", "**/*.tsx"],
    "removeZipAfterPackage": true,
    "no_git_tag": true,
    "sync_portal_details": true
  },
  "dependencies": [
    "base >= 1.1.71",
    "? PickerDollies >= 1.2.1",
    "? bobinserters >= 1.0.0", "? boblogistics >= 1.0.0", "? EditorExtensions >= 2.0.0"
  ]
}
